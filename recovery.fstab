/boot		emmc		/dev/block/mmcblk0p17
/recovery	emmc		/dev/block/mmcblk0p18				flags=backup=1
/data		ext4		/dev/block/mmcblk0p22		length=-16384
/system		ext4		/dev/block/mmcblk0p20
/cache		ext4		/dev/block/mmcblk0p19				flags=backup=1;display=cache
/efs		emmc		/dev/block/mmcblk0p14				flags=backup=1;display=EFS
/modem		emmc		/dev/block/mmcblk0p7				flags=backup=1;display="Modem"
/external_sd	vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="Micro SD";storage;wipeingui;removable
